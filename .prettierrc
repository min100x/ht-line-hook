{
  "semi": true,
  "trailingComma": "es5",
  "singleQuote": true,
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "avoid",
  "endOfLine": "lf",
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "^react$",
    "^react-(.*)$",
    "^@/(.*)$",
    "^[./]",
    "^[a-zA-Z]",
    "^@types/(.*)$",
    "^types/(.*)$",
    "^utils/(.*)$",
    "^components/(.*)$",
    "^hooks/(.*)$",
    "^services/(.*)$",
    "^api/(.*)$",
    "^config/(.*)$",
    "^constants/(.*)$",
    "^styles/(.*)$",
    "^assets/(.*)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderParserPlugins": ["typescript", "jsx", "decorators-legacy"]
}
